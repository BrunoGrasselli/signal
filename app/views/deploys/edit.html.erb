<h1>Editing deploy</h1>

<% form_for(@deploy) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :project %><br />
    <%= f.text_field :project %>
  </p>
  <p>
    <%= f.label :output %><br />
    <%= f.text_area :output %>
  </p>
  <p>
    <%= f.label :success %><br />
    <%= f.check_box :success %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @deploy %> |
<%= link_to 'Back', deploys_path %>